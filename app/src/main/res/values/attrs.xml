<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleView">
        <attr name="tv_title" format="string"></attr>

        <attr name="tv_right" format="string"></attr>

        <attr name="tv_back_visibility" format="enum">
            <enum name="visable" value="0"></enum>
            <enum name="invisable" value="1"></enum>
            <enum name ="gone" value="2"></enum>

        </attr>

        <attr name="tv_tv_right_visibility" format="enum">
            <enum name="visable" value="0"></enum>
            <enum name="invisable" value="1"></enum>
            <enum name ="gone" value="2"></enum>

        </attr>

        <attr name="tv_img_right_back_visibility" format="enum">
            <enum name="visable" value="0"></enum>
            <enum name="invisable" value="1"></enum>
            <enum name ="gone" value="2"></enum>

        </attr>
    </declare-styleable>
    
    <!--reference 表示引用  -->
    <declare-styleable name="MEditText">
        <attr name="me_lable_src" format="reference|color"/>
            
      
        <attr name="me_hint" format="string"/>
            
      <!--由代码决定-->
        <attr name="me_text" format="string"/>
        <attr name="me_del_enable" format="boolean"/>
        <attr name="me_eye_enable" format="boolean"/>
        <attr name="me_input_type" format="enum">
            <enum name="text" value="0"/>
            <enum name="password" value="1"/>
            <enum name="number" value="2"/>

        </attr>
        <attr name="me_maxlength" format="integer"/>

    </declare-styleable>
    
    
    <declare-styleable name="CricleImageView">
        <attr name="cimg_instrokewidth" format="dimension"/>
        <attr name="cimg_outstrokewidth" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="UserInfoView">
        <attr name="userinfo_img_visibility" format="enum">
            <enum name="visiable" value="0"/>
            <enum name="invisable" value="1"/>
            <enum name="gone" value="2"/>
        </attr>
        <attr name="userinfo_img_next_visibility" format="enum">
            <enum name="visiable" value="0"/>
            <enum name="invisable" value="1"/>
            <enum name="gone" value="2"/>
        </attr>
        <attr name="userinfo_et_enable" format="boolean"/>
        <attr name="userinfo_et_init" format="string|reference"/>
        <attr name="userinfo_et_input_text" format="string|reference"/>
        <attr name="usefinfo_label_text" format="string|reference"/>
    </declare-styleable>


</resources>